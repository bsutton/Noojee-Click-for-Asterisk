							</row>
<row  >
</row>

						</rows>

					</grid>
				</groupbox>

				</tabpanel>


				<tabpanel id="asterisktab">


					<groupbox>
						<caption label="Asterisk:" />

						<grid>
							<columns>
								<column width="130" />
								<column>
								</column>
							</columns>
							
							<rows>
								<row  align="center">
									<label value="Asterisk Hostname:" />
